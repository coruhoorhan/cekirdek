# Supabase Configuration
# Bu dosyayı .env olarak kopyalayın ve gerçek değerlerle doldurun
# ASLA gerçek API anahtarlarını bu dosyaya yazmayın!

# Supabase Project URL
VITE_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase Anonymous Key (Public key - frontend için güvenli)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development/Production Environment
VITE_APP_ENV=development

# Application Configuration
VITE_APP_NAME=Çekirdek Anaokulu Yönetim Sistemi
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# External Services (Gelecekte eklenecek)
# VITE_IYZICO_API_KEY=your_iyzico_key_here
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here
# VITE_FIREBASE_CONFIG=your_firebase_config_here

# Security Headers
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# Logging
VITE_LOG_LEVEL=info
